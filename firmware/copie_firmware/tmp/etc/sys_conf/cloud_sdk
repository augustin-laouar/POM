
config config 'cloud'
	option sefPort '443'
	option cloudSvrDefPort '443'
	option defaultPort '443'
	option defaultValidTime '172800'

config config 'default'
	option heartbeat_interval_ms '55000'
	option request_timeout_ms '5000'
	option max_message_number '1000'
	option max_client_number '20'
	option reconnect_timewait_max_ms '128000'
	option reconnect_cachedsvr_max_times '5'
	option reconnect_defaultsvr_max_times '5'
	option reconnect_random_time_min_ms '2000'
	option reconnect_random_time_max_ms '128000'
	option short_connect_recycle_interval_ms '259200000'
	option service_file '/etc/cloud-sdk/cloud_service.cfg'
	option cer_file '/etc/cloud-sdk/2048_newroot.cer'

